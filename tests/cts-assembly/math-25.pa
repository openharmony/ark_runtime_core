# assert(5 * 2.5 == 12.5) operation --> fmul2.64
.function u1 main(){
    fldai.64 5.0
    fmovi.64 v0, 2.5
    fmul2.64 v0
    fmovi.64 v0, 12.5
    fsub2.64 v0
    sta.64 v0
    call.short abs, v0, v0
    fmovi.64 v0, 1e-13
    fcmpl.64 v0
    jgtz exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}

.function f64 abs(f64 a0){
    fldai.64 0.0
    fcmpl.64 a0
    jlez label
    lda.64 a0
    fneg.64
    return.64
label:
    lda.64 a0
    return.64
}
