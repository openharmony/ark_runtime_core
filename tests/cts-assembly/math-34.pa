# Assert that fcmpl works fine
.function u1 main(){
    ldai.64 0x7FFFFFFFFFFFFFFF # One of valid quiet NaN values as per IEEE 754
    fmovi.64 v0, 1.1
    fcmpl.64 v0
    movi v0, -1
    jne v0, exit_failure
    fldai.64 2.1
    fmovi.64 v0, 2.2
    fcmpl.64 v0
    movi v1, -1
    jne v1, exit_failure
    fldai.64 2.3
    fcmpl.64 v0
    movi v1, 1
    jne v1, exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
