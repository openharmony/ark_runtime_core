#Assert that operation call.range works fine
.function i32 main(){
    movi v0, 1
    movi v1, 2
    call.range foo, v0
    return
}

.function i32 foo(i32 a0, i32 a1){
    ldai 1
    jeq a0, label
    return
label:
    ldai 2
    jeq a1, label2
    return
label2:
    ldai 0
    return
}
