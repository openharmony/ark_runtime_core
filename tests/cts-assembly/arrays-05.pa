#--- 
#   - title: Array length
#     description: Get length of an array and put it into accumulator.
#     instructions:
#       - sig: lenarr vs
#         format: [op_vs_8]
#         type: obj
#         dtype: i32

# assert that operation lenarr works fine

.function u1 main(){
    movi v0, 5
    newarr v1, v0, u1[]
    lenarr v1
    jne v0, exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
