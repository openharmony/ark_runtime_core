# Assert that operations ststatic, ststatic.64 work fine

.record Asm {
    i32 asm1 <static>
    i64 asm2 <static>
    f64 asm3 <static>
}

.function u1 main() {
    ldai 0xFFFFFFFF
    ststatic Asm.asm1

    ldai.64 0xFFFFFFFFFFFFFFFF
    movi.64 v0, 32
    shl2.64 v0
    ststatic.64 Asm.asm2

    fldai.64 1.0
    ststatic.64 Asm.asm3

    ldai 0
    return
}
