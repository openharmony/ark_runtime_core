# assert(5 || 2 == 7) operation --> or2.64
.function u1 main(){
    ldai.64 5
    movi.64 v0, 2
    or2.64 v0
    movi.64 v0, 7
    cmp.64 v0
    jnez exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
