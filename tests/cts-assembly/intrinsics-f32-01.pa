# Assert that intrinsic Math.fsqrt works fine
.record Math <external>
.function f32 Math.fsqrt (f32 a0) <external>

.function u1 main(){
    fmovi v0, 0.78539816339744828 # Pi / 4
    fmovi v1, 0.88622695207595825 # Expected result
    call.short Math.fsqrt, v0
    fcmpl v1
    jnez exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
