#Assert that intrinsic Math.pow works fine
.record Math <external>
.function f64 Math.pow (f64 a0, f64 a1) <external>
.function f64 Math.absF64 (f64 a0) <external>

.function u1 main(){
    fmovi.64 v0, 2.5
    fmovi.64 v1, 24.7052942201
    fmovi.64 v2, 1e-6
    fmovi.64 v3, 3.5
    call.short Math.pow, v0, v3
    fsub2.64 v1
    sta.64 v1
    call.short Math.absF64, v1, v1
    fcmpl.64 v2
    jgez exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
