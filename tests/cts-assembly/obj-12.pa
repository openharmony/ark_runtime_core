#Assert that operation ldarr.obj works fine
.record Asm{
    u1 asm1
    i32 asm2
}

.function void Asm.Construct(Asm a0) <ctor> {
    ldai 1
    stobj a0, Asm.asm1
    ldai 10
    stobj a0, Asm.asm2
    return.void
}

.function u1 main(){
    movi v0, 7
    movi v3, 2
    movi v7, 1
    movi v8, 10
    newarr v1, v0, Asm[]
    initobj.short Asm.Construct
    starr.obj v1, v3
    lda v3
    ldarr.obj v1
    sta.obj v4
    ldobj v4, Asm.asm1
    jne v7, exit_failure
    ldobj v4, Asm.asm2
    jne v8, exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
