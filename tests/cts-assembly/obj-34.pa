# check that ld.type doesn't trigger cctor

.record R1 {}

.record R2 {
    i32 s_f <static>
}

.function void R1.cctor() <cctor> {
    ldai 10
    ststatic R2.s_f
    return.void
}

.function u1 main() {
    lda.type R1
    ldstatic R2.s_f
    movi v0, 0
    jne v0, exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
