.record panda.String <external>
.record panda.NullPointerException <external>

.function panda.String panda.NullPointerException.getMessage(panda.NullPointerException a0) <external>

.record R {
    i32 f1
}

# check NullPointerException in case of load from the object

.function u1 main() {
    mov.null v0

try_begin:
    ldobj v0, R.f1
    ldai 2
    return
try_end:

catch_block1_begin:
    sta.obj v0
    call.virt panda.NullPointerException.getMessage, v0
    jnez.obj catch_block2_begin
    ldai 0
    return

catch_block2_begin:
    ldai 1
    return

.catch panda.NullPointerException, try_begin, try_end, catch_block1_begin
.catchall try_begin, try_end, catch_block2_begin
}
