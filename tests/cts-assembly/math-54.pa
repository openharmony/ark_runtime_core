# a2 - a0 * a1
.function i32 msub_i32(i32 a0, i32 a1, i32 a2) {
    mul a0, a1
    sta v1
    sub a2, v1
    return
}

# - (a0 * a1) + a2
.function i32 msub_i32_var0(i32 a0, i32 a1, i32 a2) {
    mul a0, a1
    neg
    add2 a2
    return
}

# (-a0) * a1 + a2
.function i32 msub_i32_var1(i32 a0, i32 a1, i32 a2) {
    lda a0
    neg
    mul2 a1
    add2 a2
    return
}

# a2 - a0 * a1
.function i64 msub_i64(i64 a0, i64 a1, i64 a2) {
    lda.64 a1
    mul2.64 a0
    sta.64 v0
    lda.64 a2
    sub2.64 v0
    return.64
}

# - (a0 * a1) + a2
.function i64 msub_i64_var0(i64 a0, i64 a1, i64 a2) {
    lda.64 a1
    mul2.64 a0
    neg.64
    sta.64 v0
    lda.64 a2
    add2.64 v0
    return.64
}

# (-a0) * a1 + a2
.function i64 msub_i64_var1(i64 a0, i64 a1, i64 a2) {
    lda.64 a0
    neg.64
    mul2.64 a1
    add2.64 a2
    return.64
}

