#Assert that operation modu2 works fine
.function u1 main(){
    movi v0, 3
    movi v1, 1
    ldai 7
    modu2 v0
    ucmp v1
    jnez exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
