.record panda.String <external>
.record IO <external>
.function void IO.printString(panda.String a0) <external>

# Check that arguments are passed correctly to entry point

.function u1 main(panda.String[] a0) {
    lenarr a0
    subi 2
    jnez exit_failure

    ldai 0
    ldarr.obj a0
    sta.obj v0

    ldai 1
    ldarr.obj a0
    sta.obj v1

    lda.str "\n"
    sta.obj v2

    call IO.printString, v0
    call IO.printString, v2

    call IO.printString, v1
    call IO.printString, v2

    ldai 0
    return
exit_failure:
    ldai 1
    return
}
