#Assert that intrinsic Math.cos works fine
.record Math <external>
.function f32 Math.fcos (f32 a0) <external>
.function f32 Math.absF32 (f32 a0) <external>

.function u1 main(){
    fmovi v0, 1.0
    fmovi v1, 0.5403023059
    fmovi v2, 1e-6
    call.short Math.fcos, v0, v0
    fsub2 v1
    sta v1
    call.short Math.absF32, v1, v1
    fcmpl v2
    jgez exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
