#Assert that operation isinstance works fine
.record Asm{
    i32 asm1
    i64 asm2
}
.record Asm2{
    i32 asm1
    i64 asm2
    f64 asm3
}

.function u1 main(){
    newobj v0, Asm
    lda.obj v0
    isinstance Asm
    jeqz exit_failure
    newobj v0, Asm2
    lda.obj v0
    isinstance Asm
    jnez exit_failure
    ldai 0
    return
exit_failure:
    ldai 1
    return
}
