#ifndef PANDA_RUNTIME_INCLUDE_UNIMPLEMENTED_HANDLERS_INL_H_
#define PANDA_RUNTIME_INCLUDE_UNIMPLEMENTED_HANDLERS_INL_H_

% unimplemented = %w(
%   calli.dyn.short
%   calli.dyn
% )
% unimplemented.each do |name|
%   camel_case_name = name.split('.').map { |p| p == '64' ? 'Wide' : p.capitalize }.join
